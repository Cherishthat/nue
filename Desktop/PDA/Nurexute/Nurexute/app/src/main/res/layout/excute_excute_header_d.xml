<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    <LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal" >
      <TextView
        android:id="@+id/excute_excute_head4_arcimdes"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_margin="1dp"
        android:text="@string/excute_excute_arcimdesc"
        />
    
    </LinearLayout>
    
    
    <LinearLayout 
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:orientation="horizontal" >
          <LinearLayout 
		      android:layout_width="match_parent"
		      android:layout_height="wrap_content"
		      android:layout_weight="1"
		      android:orientation="vertical" >
		       <TextView
			        android:layout_width="match_parent"
			        android:layout_height="wrap_content"
			        android:layout_margin="1dp"
			        android:text="@string/excute_excute_bedno"
			        />
		       <TextView
			        android:id="@+id/excute_excute_head4_bedno"
			        android:layout_width="match_parent"
			        android:layout_height="wrap_content"
			        android:layout_margin="1dp"
			        />
		                 
		   </LinearLayout>  
		   <LinearLayout 
		      android:layout_width="match_parent"
		      android:layout_height="wrap_content"
		      android:layout_weight="1"
		      android:orientation="vertical" >
		      <TextView
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:layout_margin="1dp"
		        android:text="@string/excute_excute_seqno"
		       />
		       <TextView
			        android:id="@+id/excute_excute_head4_seqno"
			        android:layout_width="match_parent"
			        android:layout_height="wrap_content"
			        android:layout_margin="1dp"
			        />
		                 
		   </LinearLayout>    
		   
		   <LinearLayout 
		      android:layout_width="match_parent"
		      android:layout_height="wrap_content"
		      android:layout_weight="1"
		      android:orientation="vertical" >
		      <TextView
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:layout_margin="1dp"
		        android:text="@string/excute_excute_excutenur"
		       />
		       <TextView
			        android:id="@+id/excute_excute_head4_excutnur"
			        android:layout_width="match_parent"
			        android:layout_height="wrap_content"
			        android:layout_margin="1dp"
			        />
		                 
		   </LinearLayout>  
		   
		    <LinearLayout 
		      android:layout_width="match_parent"
		      android:layout_height="wrap_content"
		      android:layout_weight="1"
		      android:orientation="vertical" >
		      <TextView
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:layout_margin="1dp"
		        android:text="@string/excute_excute_excutedt"
		       />
		       <TextView
			        android:id="@+id/excute_excute_head4_excutdt"
			        android:layout_width="match_parent"
			        android:layout_height="wrap_content"
			        android:layout_margin="1dp"
			        />
		                 
		   </LinearLayout>  
      
     </LinearLayout>
    

      <LinearLayout 
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:orientation="horizontal" >
          <LinearLayout 
		      android:layout_width="match_parent"
		      android:layout_height="wrap_content"
		      android:layout_weight="1"
		      android:orientation="vertical" >
		       <TextView
			        android:layout_width="match_parent"
			        android:layout_height="wrap_content"
			        android:layout_margin="1dp"
			        android:text="@string/excute_excute_dose"
			        />
		       <TextView
			        android:id="@+id/excute_excute_head4_dose"
			        android:layout_width="match_parent"
			        android:layout_height="wrap_content"
			        android:layout_margin="1dp"
			        />
		                 
		   </LinearLayout>
		   
           <LinearLayout 
		      android:layout_width="match_parent"
		      android:layout_height="wrap_content"
		      android:layout_weight="1"
		      android:orientation="vertical" >
		       <TextView
			        android:layout_width="match_parent"
			        android:layout_height="wrap_content"
			        android:layout_margin="1dp"
			        android:text="@string/excute_excute_meth"
			        />
		       <TextView
			        android:id="@+id/excute_excute_head4_meth"
			        android:layout_width="match_parent"
			        android:layout_height="wrap_content"
			        android:layout_margin="1dp"
			        />
		                 
		   </LinearLayout>
		   
            <LinearLayout 
		      android:layout_width="match_parent"
		      android:layout_height="wrap_content"
		      android:layout_weight="1"
		      android:orientation="vertical" >
		       <TextView
			        android:layout_width="match_parent"
			        android:layout_height="wrap_content"
			        android:layout_margin="1dp"
			        android:text="@string/excute_excute_phfreq"
			        />
		       <TextView
			        android:id="@+id/excute_excute_head4_phrreq"
			        android:layout_width="match_parent"
			        android:layout_height="wrap_content"
			        android:layout_margin="1dp"
			        />
		                 
		   </LinearLayout>
		   
            <LinearLayout 
		      android:layout_width="match_parent"
		      android:layout_height="wrap_content"
		      android:layout_weight="1"
		      android:orientation="vertical" >
		       <TextView
			        android:layout_width="match_parent"
			        android:layout_height="wrap_content"
			        android:layout_margin="1dp"
			        android:text="@string/excute_excute_priority"
			        />
		       <TextView
			        android:id="@+id/excute_excute_head4_priority"
			        android:layout_width="match_parent"
			        android:layout_height="wrap_content"
			        android:layout_margin="1dp"
			        />
		                 
		   </LinearLayout>
		   
           
	</LinearLayout>
	

</LinearLayout>







