<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/menu_back_group"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="45dp"
        android:layout_marginTop="30dp"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/LinearLayout01"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <Button
                android:id="@+id/menu_patientlist_btn"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:layout_marginRight="2dp"
                android:layout_weight="1.00"
                android:background="@drawable/menu_btn_selector"
                android:text="@string/menu_btn_patintlist"
                android:textColor="@color/white" />

            <Button
                android:id="@+id/menu_vitalsigns"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:layout_marginLeft="2dp"
                android:layout_weight="1.00"
                android:background="@drawable/menu_btn_selector"
                android:text="@string/menu_btn_vitalsigns"
                android:textColor="@color/white" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/LinearLayout02"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <Button
                android:id="@+id/menu_linchuangshuye"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:layout_marginRight="2dp"
                android:layout_marginTop="4dp"
                android:layout_weight="1.00"
                android:background="@drawable/menu_btn_selector"
                android:text="@string/menu_btn_linchuangshuye"
                android:textColor="@color/white" />

            <Button
                android:id="@+id/menu_hushizhixing"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="4dp"
                android:layout_weight="1.00"
                android:background="@drawable/menu_btn_selector"
                android:text="@string/menu_btn_hushizhix"
                android:textColor="@color/white" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/LinearLayout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <Button
                android:id="@+id/menu_caijibiaoben"
                android:layout_width="120dp"
                android:layout_height="76dp"
                android:layout_marginTop="4dp"
                android:layout_weight="1"
                android:background="@drawable/menu_btn_selector"
                android:text="@string/menu_btn_caijibiaoben"
                android:textColor="@color/white" />
        </LinearLayout>

    </LinearLayout>

</LinearLayout>
